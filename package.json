{
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md,json}\"",
    "prepare": "husky install",
    "precommit": "lint-staged --relative"
  },
  "devDependencies": {
    "eslint": "8.57.0",
    "husky": "9.0.11",
    "prettier": "3.1.1",
    "tsconfig": "workspace:*",
    "turbo": "latest"
  },
  "packageManager": "pnpm@8.6.10",
  "name": "monorepo-template"
}
